{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".sanity/**", "dist/**"]
    },
    "@portfolio/site#build": {
      "dependsOn": ["@portfolio/cms#build"],
      "outputs": [".svelte-kit/**"]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
